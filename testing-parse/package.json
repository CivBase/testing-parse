{
  "name": "testing-parse",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CivBase/testing-parse"
  },
  "main": "static/main.jsx",
  "scripts": {
    "lint": "eslint src/ test/ --ext .jsx,.js && jshint . && jscs .",
    "postinstall": "npm run build",
    "build": "webpack -p --config webpack.production.config.js",
    "test": "PHANTOMJS_BIN=./node_modules/.bin/phantomjs && ./node_modules/karma/bin/karma start karma.config.js",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "server": "node server.js",
    "clean": "rm -r build/"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "istanbul": "^0.3.19",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "karma": "^0.12.37",
    "karma-coverage": "^0.2.7",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-sinon-chai": "^0.3.2",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "basscss": "^7.0.4",
    "css-loader": "^0.17.0",
    "cssnext-loader": "^1.0.1",
    "express": "^4.13.3",
    "parse": "^1.5.0",
    "parse-react": "^0.4.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "webpack": "^1.12.1"
  }
}
