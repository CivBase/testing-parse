{
  "name": "testing-parse",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CivBase/testing-parse"
  },
  "main": "static/main.jsx",
  "scripts": {
    "lint": "eslint src/ test/ --ext .jsx,.js && jshint . && jscs .",
    "postinstall": "npm run build",
    "build": "webpack -p --config webpack.production.config.js",
    "test": "PHANTOMJS_BIN=./node_modules/.bin/phantomjs && ./node_modules/karma/bin/karma start karma.config.js",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "server": "node server.js",
    "clean": "rm build/app.js",
    "superclean": "npm run clean && rm -r node_modules && npm install"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "eslint": "^1.4.1",
    "istanbul": "^0.3.20",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon-chai": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "express": "^4.13.3",
    "history": "^1.9.0",
    "jquery": "^2.1.4",
    "parse": "^1.6.3",
    "parse-react": "^0.5.0",
    "react": "^0.13.3",
    "react-router": "^1.0.0-rc1",
    "webpack": "^1.12.1"
  }
}
